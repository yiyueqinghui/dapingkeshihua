* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

html,
body {
  width: 100%;
  height: 100%;
}

/* 图标样式 */
.iconfont {
  color: #585151;
}

.icon_ffffff {
  color: #ffffff;
}
.self_scroll{
  overflow-y: auto;
}
/* 滚动条样式 */
.self_scroll::-webkit-scrollbar {
  width: 6px;
  height:6px;
}

/* 修改 滚动条的 下面 的 样式 */
.self_scroll::-webkit-scrollbar-track {
  background-color: transparent;
}

/* 修改 滑块 */
.self_scroll::-webkit-scrollbar-thumb {
    border-radius: 4em;
    background-color: rgba(144,146,152,.8);
}
